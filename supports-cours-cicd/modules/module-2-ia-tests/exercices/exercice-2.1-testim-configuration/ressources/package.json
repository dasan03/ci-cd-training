{
  "name": "testim-ai-exercise",
  "version": "1.0.0",
  "description": "Exercice pratique Testim avec IA",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "testim run --token $TESTIM_TOKEN --project $TESTIM_PROJECT_ID",
    "test:ci": "testim run --token $TESTIM_TOKEN --project $TESTIM_PROJECT_ID --suite regression-suite --parallel 3"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "bcrypt": "^5.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@testim/testim-cli": "^4.15.0",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "testim",
    "ai-testing",
    "automation",
    "e2e-testing"
  ],
  "author": "Formation CI/CD",
  "license": "MIT"
}